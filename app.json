{
	"expo": {
		"owner": "okane-app",
		"name": "Okane",
		"slug": "okane",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover"
		},
